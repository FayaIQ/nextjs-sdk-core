{
  "name": "my-next-core",
  "version": "1.0.0",
  "type": "module",
  "description": "A Next.js SDK for server and client components with simple API functions",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/FayaIQ/nextjs-sdk-core.git"
  },
  "bugs": {
    "url": "https://github.com/FayaIQ/nextjs-sdk-core/issues"
  },
  "homepage": "https://github.com/FayaIQ/nextjs-sdk-core#readme",
  "bin": {
    "my-next-core": "./bin/setup.js"
  },
  "files": [
    "dist",
    "bin",
    "README_PACKAGE.md",
    "QUICK_START.md",
    ".env.example"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/core/index.ts src/identity/index.ts src/identity/server.ts src/inventory/orders/index.ts src/handlers/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts src/core/index.ts src/identity/index.ts src/identity/server.ts src/inventory/orders/index.ts src/handlers/index.ts --format cjs,esm --dts --watch",
    "prepublishOnly": "npm run build"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js",
      "require": "./dist/core/index.cjs"
    },
    "./identity": {
      "types": "./dist/identity/index.d.ts",
      "import": "./dist/identity/index.js",
      "require": "./dist/identity/index.cjs"
    },
    "./identity/server": {
      "types": "./dist/identity/server.d.ts",
      "import": "./dist/identity/server.js",
      "require": "./dist/identity/server.cjs"
    },
    "./identity/*": {
      "types": "./dist/identity/*.d.ts",
      "import": "./dist/identity/*.js",
      "require": "./dist/identity/*.cjs"
    },
    "./handlers": {
      "types": "./dist/handlers/index.d.ts",
      "import": "./dist/handlers/index.js",
      "require": "./dist/handlers/index.cjs"
    },
    "./handlers/*": {
      "types": "./dist/handlers/*.d.ts",
      "import": "./dist/handlers/*.js",
      "require": "./dist/handlers/*.cjs"
    },
    "./inventory/orders": {
      "types": "./dist/inventory/orders/index.d.ts",
      "import": "./dist/inventory/orders/index.js",
      "require": "./dist/inventory/orders/index.cjs"
    },
    "./inventory/orders/*": {
      "types": "./dist/inventory/orders/*.d.ts",
      "import": "./dist/inventory/orders/*.js",
      "require": "./dist/inventory/orders/*.cjs"
    }
  },
  "peerDependencies": {
    "next": "^14.0.0 || ^15.0.0 || ^16.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "nextjs",
    "sdk",
    "storeak",
    "api",
    "server-components",
    "client-components",
    "react",
    "typescript"
  ]
}
