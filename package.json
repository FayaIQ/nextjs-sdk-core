      "import": "./dist/inventory/menus/index.js",
      "require": "./dist/inventory/menus/index.cjs"
    },
    "./inventory/menus/*": {
      "types": "./dist/inventory/menus/*.d.ts",
      "import": "./dist/inventory/menus/*.js",
      "require": "./dist/inventory/menus/*.cjs"
    },
    "./inventory/offers": {
      "types": "./dist/inventory/offers/index.d.ts",
      "import": "./dist/inventory/offers/index.js",
      "require": "./dist/inventory/offers/index.cjs"
    },
    "./inventory/offers/*": {
      "types": "./dist/inventory/offers/*.d.ts",
      "import": "./dist/inventory/offers/*.js",
      "require": "./dist/inventory/offers/*.cjs"
    },
    "./inventory/category": {
      "types": "./dist/inventory/category/index.d.ts",
      "import": "./dist/inventory/category/index.js",
      "require": "./dist/inventory/category/index.cjs"
    },
    "./inventory/category/*": {
      "types": "./dist/inventory/category/*.d.ts",
      "import": "./dist/inventory/category/*.js",
      "require": "./dist/inventory/category/*.cjs"
    },
    "./inventory/itemSource": {
      "types": "./dist/inventory/itemSource/index.d.ts",
      "import": "./dist/inventory/itemSource/index.js",
      "require": "./dist/inventory/itemSource/index.cjs"
    },
    "./inventory/itemSource/*": {
      "types": "./dist/inventory/itemSource/*.d.ts",
      "import": "./dist/inventory/itemSource/*.js",
      "require": "./dist/inventory/itemSource/*.cjs"
    },
    "./inventory/orderItem": {
      "types": "./dist/inventory/orderItem/index.d.ts",
      "import": "./dist/inventory/orderItem/index.js",
      "require": "./dist/inventory/orderItem/index.cjs"
    },
    "./inventory/orderItem/*": {
      "types": "./dist/inventory/orderItem/*.d.ts",
      "import": "./dist/inventory/orderItem/*.js",
      "require": "./dist/inventory/orderItem/*.cjs"
    }
  },
  "peerDependencies": {
    "next": "^14.0.0 || ^15.0.0 || ^16.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "nextjs",
    "sdk",
    "storeak",
    "api",
    "server-components",
    "client-components",
    "react",
    "typescript"
  ]
}
