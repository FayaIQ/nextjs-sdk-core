      "require": "./dist/inventory/reports/index.cjs"
    },
    "./inventory/reports/*": {
      "types": "./dist/inventory/reports/*.d.ts",
      "import": "./dist/inventory/reports/*.js",
      "require": "./dist/inventory/reports/*.cjs"
    },
    "./inventory/slides/getSlides": {
      "types": "./dist/inventory/slides/getSlides.d.ts",
      "import": "./dist/inventory/slides/getSlides.js",
      "require": "./dist/inventory/slides/getSlides.cjs"
    },
    "./inventory/items": {
      "types": "./dist/inventory/items/index.d.ts",
