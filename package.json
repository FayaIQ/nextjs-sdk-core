{
  "name": "erp-core",
  "version": "1.0.0",
  "type": "module",
  "description": "A Next.js SDK for server and client components with simple API functions",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/FayaIQ/nextjs-sdk-core.git"
  },
  "bugs": {
    "url": "https://github.com/FayaIQ/nextjs-sdk-core/issues"
  },
  "homepage": "https://github.com/FayaIQ/nextjs-sdk-core#readme",
  "bin": {
    "my-next-core": "./bin/setup.js"
  },
  "files": [
    "dist",
    "bin",
    "README_PACKAGE.md",
    "QUICK_START.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/core/index.ts src/identity/index.ts src/identity/server.ts src/inventory/orders/index.ts src/inventory/items/index.ts src/inventory/orderItem/index.ts src/inventory/menus/index.ts src/inventory/offers/index.ts src/inventory/category/index.ts src/inventory/itemSource/index.ts src/gps/locations/index.ts src/identity/application/index.ts  --format cjs,esm --dts",
    "dev": "tsup src/index.ts src/core/index.ts src/identity/index.ts src/identity/server.ts src/inventory/orders/index.ts src/inventory/items/index.ts src/inventory/orderItem/index.ts src/inventory/menus/index.ts src/inventory/offers/index.ts src/inventory/category/index.ts src/inventory/itemSource/index.ts src/identity/application/index.ts src/gps/locations/index.ts  --format cjs,esm --dts --watch",
    "prepublishOnly": "npm run build"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js",
      "require": "./dist/core/index.cjs"
    },
    "./gps": {
      "types": "./dist/gps/index.d.ts",
      "import": "./dist/gps/index.js",
      "require": "./dist/gps/index.cjs"
    },
    "./gps/*": {
      "types": "./dist/gps/*.d.ts",
      "import": "./dist/gps/*.js",
      "require": "./dist/gps/*.cjs"
    },
    "./gps/locations": {
      "types": "./dist/gps/locations/index.d.ts",
      "import": "./dist/gps/locations/index.js",
      "require": "./dist/gps/locations/index.cjs"
    },
    "./gps/locations/*": {
      "types": "./dist/gps/locations/*.d.ts",
      "import": "./dist/gps/locations/*.js",
      "require": "./dist/gps/locations/*.cjs"
    },
    "./identity": {
      "types": "./dist/identity/index.d.ts",
      "import": "./dist/identity/index.js",
      "require": "./dist/identity/index.cjs"
    },
    "./identity/*": {
      "types": "./dist/identity/*.d.ts",
      "import": "./dist/identity/*.js",
      "require": "./dist/identity/*.cjs"
    },
    "./identity/application": {
      "types": "./dist/identity/application/index.d.ts",
      "import": "./dist/identity/application/index.js",
      "require": "./dist/identity/application/index.cjs"
    },
    "./identity/application/*": {
      "types": "./dist/identity/application/*.d.ts",
      "import": "./dist/identity/application/*.js",
      "require": "./dist/identity/application/*.cjs"
    },
    "./inventory/orders": {
      "types": "./dist/inventory/orders/index.d.ts",
      "import": "./dist/inventory/orders/index.js",
      "require": "./dist/inventory/orders/index.cjs"
    },
    "./inventory/orders/*": {
      "types": "./dist/inventory/orders/*.d.ts",
      "import": "./dist/inventory/orders/*.js",
      "require": "./dist/inventory/orders/*.cjs"
    },
    "./inventory/payments": {
      "types": "./dist/inventory/payments/index.d.ts",
